            let htmlString = "";
            $.each(list, (index, value) => {
                htmlString += "<tr>";
                htmlString += "	<td>";
                htmlString += "		" + value.playName;
                htmlString += "	</td>";
                htmlString += "	<td>";
                htmlString += "		" + value.gameName;
                htmlString += "	</td>";
                htmlString += "	<td>";
                htmlString += "		" + value.endDateTime ? gfn_timestampToDate(value.endDateTime) : "-";
                htmlString += "	</td>";
                htmlString += "</tr>";
            });

            gfn_removeElementChildrenAndAppendHtmlString($tbody, htmlString);

            const params = {
                pagingObject: $div.find("ul.pagination"),
                pageIndex: "pageIndex",
                totalCount: data.totalElements,
                eventName: "clubProfileModal.readClubActivityPageById",
                recordCount: clubProfileModal.PAGE_SIZE,
            };

            gfn_renderPaging(params);
        },
        readClubPlayImagePageById: page => {
            const request = {
                clubId: clubProfileModal.clubId,
                page,
                size: clubProfileModal.PAGE_SIZE,
                descending: true,
                sortBy: "endDateTime",
            }

            gfn_callGetApi("/api/club/play/image/page", request)
                .then(data => clubProfileModal.readClubPlayImagePageByIdCallback(data))
                .catch(response => console.error('error', response));
        },
        readClubPlayImagePageByIdCallback: data => {
            const $div = $("#clubPlayImageDiv");
            const $tbody = $div.find("tbody");

            const list = data.content;
            if (list.length === 0) {
                $("#clubPlayImageDiv").hide();
                return;
            }

            let htmlString = "";
            htmlString += "<hr class=\"my-4\" />";
            htmlString += "<h5>플레이소감</h5>";
            htmlString += "<div class=\"row clearfix\">";
            $.each(list, (index, value) => {
                htmlString += "<div class=\"col-lg-12 mb-4\">";
                htmlString += "	<div class=\"card\">";
                htmlString += "		<img src=\"https://bogopayo.cafe24.com/img/play/" + value.seq + "/" + value.fdbckImgFileNm + "\" class=\"card-img-top\">";
                htmlString += "		<div class=\"card-body\">";
                htmlString += "			<p class=\"card-text\">\"" + value.fdbck + "\"</p>";
                htmlString += "			<div class=\"card-text-footer\">" + value.nickName + ", <cite>" + value.endDt + " " + value.gameNm + " " + value.rsltRnk + "등</cite></div>";
                htmlString += "		</div>";
                htmlString += "	</div>";
                htmlString += "</div>";
            });
            htmlString += "</div>";

            gfn_removeElementChildrenAndAppendHtmlString($tbody, htmlString);

            const params = {
                pagingObject: $div.find("ul.pagination"),
                pageIndex: "pageIndex",
                totalCount: data.totalElements,
                eventName: "clubProfileModal.readClubPlayImagePageById",
                recordCount: clubProfileModal.PAGE_SIZE,
            };

            gfn_renderPaging(params);

            $("#clubPlayImageDiv").show();
        },

        


