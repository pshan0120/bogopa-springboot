<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<script>
    $(() => {
        const $modal = $("#clubProfileModal");
        const $tablistUl = $modal.find("ul[name='tablistUl']");

        $tablistUl.find("a").click((e) => {
            const $this = $(e.currentTarget);
            const tabId = $this.data("tabId");
            console.log('tabId', tabId);

            if (tabId === "info") {
                clubProfileModal.readInfoById(clubProfileModal.clubId);
            }

            if (tabId === "member") {
                clubProfileModal.readClubMemberPageById(1);
            }

            if (tabId === "game") {
                clubProfileModal.readClubGamePageById(1);
            }

            if (tabId === "activity") {
                clubProfileModal.readClubActivityPageById(1);
            }
        });
    });

    const clubProfileModal = {
        clubId: null,
        PAGE_SIZE: 5,
        open: clubId => {
            clubProfileModal.clubId = clubId;

            const $modal = $("#clubProfileModal");
            $modal.modal("show");

            const $tablistUl = $modal.find("ul[name='tablistUl']");
            $tablistUl.find("a:first").click();
        },
        readInfoById: clubId => {
            gfn_callGetApi("/api/club/info", {clubId})
                .then(data => clubProfileModal.readInfoByIdCallback(data))
                .catch(response => console.error('error', response));
        },
        readInfoByIdCallback: data => {
            const $form = $("#clubInfoDiv").find("form");
            $form.find("img[name='clubProfileImage']").attr("src", createClubImageUrl(data.clubId, data.profileImageFileName));
            $form.find("input[name='clubName']").val(data.clubName);
            $form.find("input[name='masterMemberName']").val(data.masterMemberName);
            $form.find("input[name='clubGradeName']").val(ClubGrade.ofCode(data.clubGradeCode).title);
            $form.find("input[name='contactTo']").val(data.contactTo);
            $form.find("input[name='address']").val(data.address);
            $form.find("textarea[name='introduction']").val(data.introduction);
            $form.find("textarea[name='joinQuestion']").val(data.joinQuestion);

            $("#showClubJoinButton").hide();
            $("#clubJoinButton").hide();
            $("#cancelClubJoinButton").hide();
            $("#quitClubButton").hide();
            $("#disbandClubButton").hide();

            const loggedIn = JSON.parse("<%= SessionUtils.isMemberLogin() %>");
            if (!loggedIn) {
                return;
            }

            const memberId = JSON.parse("<%= SessionUtils.getCurrentMemberIdOrNull() %>");
            if (data.masterMemberId === memberId) {
                $("#disbandClubButton").show();
                return;
            }

            if (data.memberClubJoinRequested) {
                $("#cancelClubJoinButton").show();
                return;
            }

            if (data.clubJoined) {
                $("#quitClubButton").show();
                return;
            }

            $("#showClubJoinButton").show();
        },
        showClubJoin: () => {
            $("#showClubJoinButton").hide();

            const $clubInfoDiv = $("#clubInfoDiv");
            $clubInfoDiv.find("div[name='showClubJoinDiv']").show();
            $("#clubJoinButton").show();
        },
        readClubMemberPageById: page => {
            const request = {
                clubId: clubProfileModal.clubId,
                page,
                size: clubProfileModal.PAGE_SIZE,
                descending: true,
                sortBy: "clubMemberGradeCode",
            }

            gfn_callGetApi("/api/club/member/page", request)
                .then(data => clubProfileModal.readClubMemberPageByIdCallback(data))
                .catch(response => console.error('error', response));
        },
        readClubMemberPageByIdCallback: data => {
            const $div = $("#clubMemberDiv");
            const $tbody = $div.find("tbody");

            const list = data.content;
            if (list.length === 0) {
                const htmlString = "<tr><td colspan='4' class=\"text-center\">조회결과가 없습니다.</td></tr>";
                gfn_removeElementChildrenAndAppendHtmlString($tbody, htmlString);
                return;
            }

            let htmlString = "";
            $.each(list, (index, value) => {
                htmlString += "<tr>";
                htmlString += "	<td scope=\"row\">";
                htmlString += "		<div class=\"media align-items-center\">";
                htmlString += "			<span class=\"avatar avatar-sm rounded-circle mr-3\" >";
                htmlString += "			<img src=\"" + createMemberImageUrl(data.memberId, data.profileImageFileName) + "\" class=\"rounded-circle\">";
                htmlString += "			</span>";
                htmlString += "			<div class=\"media-body\">";
                htmlString += "				<span class=\"mb-0 text-sm\">" + value.nickName + "</span>";
                htmlString += "			</div>";
                htmlString += "		</div>";
                htmlString += "	</td>";
                htmlString += "	<td>";
                htmlString += "		" + ClubMemberGrade.ofCode(value.clubMemberGradeCode).title;
                htmlString += "	</td>";
                htmlString += "	<td>";
                htmlString += "		" + value.joinedAt;
                htmlString += "	</td>";
                htmlString += "	<td>";
                htmlString += "		" + gfn_comma(value.playCount);
                htmlString += "	</td>";
                htmlString += "</tr>";
            });

            gfn_removeElementChildrenAndAppendHtmlString($tbody, htmlString);

            const params = {
                pagingObject: $div.find("ul.pagination"),
                pageIndex: "pageIndex",
                totalCount: data.totalElements,
                eventName: "clubProfileModal.readClubMemberPageById",
                recordCount: clubProfileModal.PAGE_SIZE,
            };

            gfn_renderPaging(params);
        },
        readClubGamePageById: page => {
            const request = {
                clubId: clubProfileModal.clubId,
                page,
                size: clubProfileModal.PAGE_SIZE,
                descending: true,
                sortBy: "numberOfPlayed",
            }

            gfn_callGetApi("/api/club/game/page", request)
                .then(data => clubProfileModal.readClubGamePageByIdCallback(data))
                .catch(response => console.error('error', response));
        },
        readClubGamePageByIdCallback: data => {
            const $div = $("#clubGameDiv");
            const $tbody = $div.find("tbody");

            const list = data.content;
            if (list.length === 0) {
                const htmlString = "<tr><td colspan='3' class=\"text-center\">조회결과가 없습니다.</td></tr>";
                gfn_removeElementChildrenAndAppendHtmlString($tbody, htmlString);
                return;
            }

            let htmlString = "";
            $.each(list, (index, value) => {
                htmlString += "<tr>";
                htmlString += "	<td>";
                htmlString += "		" + value.gameName;
                htmlString += "	</td>";
                htmlString += "	<td>";
                htmlString += "		" + value.numberOfMinPlayer + " ~ " + value.numberOfMaxPlayer;
                htmlString += "	</td>";
                htmlString += "	<td>";
                htmlString += "		" + gfn_comma(value.numberOfPlayed);
                htmlString += "	</td>";
                htmlString += "</tr>";
            });

            gfn_removeElementChildrenAndAppendHtmlString($tbody, htmlString);

            const params = {
                pagingObject: $div.find("ul.pagination"),
                pageIndex: "pageIndex",
                totalCount: data.totalElements,
                eventName: "clubProfileModal.readClubGamePageById",
                recordCount: clubProfileModal.PAGE_SIZE,
            };

            gfn_renderPaging(params);
        },
        readClubActivityPageById: page => {
            const request = {
                clubId: clubProfileModal.clubId,
                page,
                size: clubProfileModal.PAGE_SIZE,
                descending: true,
                sortBy: "endDateTime",
            }

            gfn_callGetApi("/api/club/activity/page", request)
